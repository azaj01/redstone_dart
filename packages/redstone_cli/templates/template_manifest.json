{
  "version": 1,
  "templates": {
    "mod": {
      "description": "Standard Redstone mod with dual-runtime (server+client) structure",
      "files": [
        "pubspec.yaml.tmpl",
        "redstone.yaml.tmpl",
        ".vscode/settings.json.tmpl",
        "packages/server/pubspec.yaml.tmpl",
        "packages/server/lib/main.dart.tmpl",
        "packages/client/pubspec.yaml.tmpl",
        "packages/client/lib/main.dart.tmpl",
        "packages/common/pubspec.yaml.tmpl",
        "packages/common/lib/__project_name___common.dart.tmpl",
        "packages/server/test/hello_block_test.dart.tmpl",
        "README.md.tmpl",
        ".gitignore.copy.tmpl",
        "analysis_options.yaml.copy.tmpl",
        "minecraft/build.gradle.tmpl",
        "minecraft/settings.gradle.tmpl",
        "minecraft/gradle.properties.tmpl",
        "minecraft/src/main/resources/fabric.mod.json.tmpl",
        "minecraft/src/main/resources/__project_name__.mixins.json.tmpl"
      ]
    },
    "mod_empty": {
      "description": "Minimal Redstone mod without example code",
      "extends": "mod",
      "overrides": [
        "packages/server/lib/main.dart.tmpl",
        "packages/client/lib/main.dart.tmpl"
      ]
    }
  }
}
